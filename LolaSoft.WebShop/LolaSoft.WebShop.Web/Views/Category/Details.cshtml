@model DetailsCategoryViewModel
@{
    ViewData["Title"] = "Details category";
}

<h2>Details of category</h2>

<div class="form-group">
    @Html.LabelFor(m => m.Id, new {@class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.TextBoxFor(m => m.Id, new {@readonly = "readonly" })
    </div>
</div>

<div class="form-group">
    @Html.LabelFor(m => m.Name, new {@class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.TextBoxFor(m => m.Name, new {@readonly = "readonly" })
    </div>
</div>

<div class="form-group">
    @Html.LabelFor(m => m.ParentCategoryName, new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.TextBoxFor( m => m.ParentCategoryName, new {@readonly = "readonly" })
    </div>
</div>

